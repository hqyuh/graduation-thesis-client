{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint . --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext=js,jsx,ts,tsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext=js,ts,tsx --fix --ignore-path .gitignore",
    "format": "prettier --write **/*.{ts,tsx,css,md,json} --ignore-path .gitignore",
    "format:check": "prettier --check **/*.{js,jsx,ts,tsx,css,md,json} --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/react-modal": "^3.13.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "axios": "^0.26.1",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "formik": "^2.2.9",
    "next": "12.1.0",
    "nextjs-progressbar": "^0.0.14",
    "primeicons": "^5.0.0",
    "primereact": "^7.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-modal": "^3.14.4",
    "react-redux": "^7.2.6",
    "react-toastify": "^8.2.0",
    "react-transition-group": "^4.4.2",
    "sass": "^1.49.9",
    "yup": "^0.32.11"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint-config-kinal"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/jest": "^27.4.1",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "eslint": "8.10.0",
    "eslint-config-kinal": "^1.0.3",
    "eslint-config-next": "12.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "4.6.2"
  },
  "testEnvironment": "jsdom"
}
